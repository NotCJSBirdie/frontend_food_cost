{
  "version": 2,
  "framework": "vite",
  "builds": [
    {
      "src": "dist/index.html",
      "use": "@vercel/static",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
